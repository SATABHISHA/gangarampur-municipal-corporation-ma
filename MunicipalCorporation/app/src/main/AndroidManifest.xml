<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.municipal.myapplication">
<uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".sr.home.ActivityMain"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sr.home.ActivityHome"/>
        <activity android:name=".sr.Complaint.ComplainActivityHome"/>
        <activity android:name=".sr.home.ActivityNewsHome"/>
        <activity android:name=".sr.notice.NoticeActivityHome"/>
        <activity android:name=".sr.event.EventActivityHome"/>
        <activity android:name=".sr.tender.TenderActivityHome"/>
        <activity android:name=".sr.career.CareerActivityHome"/>
        <activity android:name=".sr.facility.FacilityActivityHome"/>
        <activity android:name=".sr.recentwork.RecentWorkActivityHome"/>
    </application>

</manifest>